    "marked": "^1.2.2",
    "mitt": "^2.1.0",
    "reflect-metadata": "0.1.13",
    "typescript": "^4.0.3",
    "vue": "^3.0.0-rc.1",
    "vue-router": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.98",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "vite": "^1.0.0-rc.1",
    "autoprefixer": "^9.8.5",
    "cssnano": "^4.1.10",
    "rollup": "^2.22.1",

{
  "name": "wike-ui",
  "version": "0.0.1",
  "description": "一个基于vue3和DDD驱动的项目",
  "repository": {
    "type": "url",
    "url": "https://github.com/wike2019/wike-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wike2019/wike-ui/issues"
  },
  "keywords": [
    "wike-ui",
    "vue-next",
    "vue3",
    "common component",
    "ddd"
  ],
  "scripts": {
